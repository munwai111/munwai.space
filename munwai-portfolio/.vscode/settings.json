{
  "css.enabledLanguages": [
    "html",
    "javascript",
    "typescript",
    "jsx",
    "tsx",
    "css",
    "json"
  ],
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "css.validate": true,
  "less.validate": true,
  "scss.validate": true,
  "emmet.includeLanguages": {
    "javascript": "html",
    "jsx": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.js": "javascript"
  }
}
